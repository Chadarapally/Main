{
	"name": "Copyfile_zip_unzip",
	"properties": {
		"description": "copying json zip file read",
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
